{
  "name": "Staff Pricing Calculator",
  "version": "1.0",
  "iconURL": "https://servicem8-pricing-addon.onrender.com/icon.png",
  "supportURL": "https://github.com/mrg1989/servicem8-pricing-addon",
  "supportEmail": "marvin@freeflo.co.uk",
  "oauth": {
    "scope": "read_jobs write_jobs read_clients"
  },
  "actions": [
    {
      "name": "Calculate Pricing",
      "type": "online",
      "entity": "job",
      "iconURL": "https://servicem8-pricing-addon.onrender.com/icon.png",
      "event": "calculate_job_pricing",
      "location": "modal"
    }
  ],
  "menuItems": [
    {
      "name": "Pricing Calculator",
      "type": "addon",
      "iconURL": "https://servicem8-pricing-addon.onrender.com/icon.png",
      "event": "pricing_calculator_menu"
    }
  ],
  "webhooks": [
    {
      "object": "job",
      "fields": [
        "job_address",
        "billing_address",
        "generated_job_id",
        "job_description",
        "total_cost"
      ]
    }
  ]
}
